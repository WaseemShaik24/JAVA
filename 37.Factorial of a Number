import java.util.Scanner;

public class Factorial {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int n = s.nextInt();
        long f = 1;

        for (int i = 2; i <= n; i++) {
            f *= i;
        }

        System.out.println(n + " Factorial = " + f);
    }
}
