import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        System.out.println("Enter the Number:'");
        int n = s.nextInt();
        boolean isPowerOfThree = n > 0 && (Math.log10(n) / Math.log10(3)) % 1 == 0;
        System.out.println(isPowerOfThree);
    }
}
