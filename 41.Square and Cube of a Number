import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        System.out.println("Enter the Number:");
        double n = s.nextDouble();
        System.out.println("Square: " + (n * n));
        System.out.println("Cube: " + (n * n * n));
    }
}
