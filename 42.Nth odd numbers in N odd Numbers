import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int n = s.nextInt();
        for (int i = 1, c = 0; c < n; i += 2, c++) {
            System.out.print(i + " ");
        }
        
        int nthOdd = 2 * n - 1;
        System.out.println("\nThe nth odd number after the first " + n + " odd numbers is: " + nthOdd);
    }
}
