import java.util.Scanner;
public class Pattern{
    public static void main(String[] args) {
        Scanner obj = new Scanner(System.in);
        System.out.print("Enter the number of times to be printed:");
        int n=obj.nextInt();
        int x=1;
        for(int i=1;i<=n;i++){
            for(int j=1;j<=i;j++){
                System.out.print(x*x+" ");
                x++;
            }
            System.out.println();
        }
    }
}
